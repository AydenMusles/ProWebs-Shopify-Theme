        }

        &.banner-page {
            height: auto;
            min-height: 100%;
            padding-top: 100px;
            padding-bottom: 30px;
        }

        .banner--container {
            max-width: 640px;
        }
        .banner--container--large {
            max-width: 830px;
        }

        .banner--image {
          position: absolute;
          width: 100%;
          height: 100%;
